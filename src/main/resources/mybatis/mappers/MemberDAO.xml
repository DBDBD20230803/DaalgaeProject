<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.daalgae.daalgaeproject.member.model.dao.MemberDAO">
    <insert id="registMember" parameterType="MemberDTO">
        INSERT
            INTO MEM_INFO       (
                                 MEM_ID
                                 ,MEM_PWD
                                 ,MEM_NAME
                                 ,MEM_BIRTH
                                 ,MEM_EMAIL
                                 ,MEM_ADRS
                                 ,MEM_ADRS_DETAIL
                                 )
        VALUES (
                #{ memId }
                ,#{ memPwd}
                ,#{ memName }
                ,#{ memBirth }
                ,{ memEmail }
                ,#{ memAdres }
                ,#{ memAdresDetail }
               )
    </insert>
</mapper>