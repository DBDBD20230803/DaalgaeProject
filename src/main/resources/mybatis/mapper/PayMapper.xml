<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.daalgae.daalgaeproject.payment.dao.OrderPayMapper">


    <resultMap id="approveResultMap" type="com.daalgae.daalgaeproject.payment.dto.OrderPay">
        <id property="tid" column="DOG_GUM_PAY_CODE"/>
        <result property="dogGumPaymethod" column="DOG_GUM_PAYMETHOD"/>
        <result property="dogGumPay" column="DOG_GUM_PAY"/>
        <result property="memDogGum" column="MEM_DOG_GUM"/>
        <result property="dogItemName" column="DOG_ITEM_NAME"/>
        <result property="memCode" column="REF_MEM_CODE"/>
    </resultMap>

    <!--<select id="findAllPay" resultMap="approveResultMap">
            SELECT DOG_GUM_PAY_CODE
                    , MEM_CODE
        FROM DOG_GUM_PAY

    </select>-->

    <insert id="orderRegist" parameterType="com.daalgae.daalgaeproject.payment.dto.OrderPay">
        INSERT INTO DOG_GUM_PAY (
                                DOG_GUM_PAY_CODE
                                ,DOG_GUM_PAYMETHOD
                                ,DOG_GUM_PAY
                                ,DOG_GUM_PAY_DATE
                                ,DOG_ITEM_NAME
                                ,MEM_DOG_GUM
                                ,MEM_CODE
                            )
        VALUES (
                #{ dogGumPayCode },
                #{ dogGumPaymethod },
                #{ dogGumPay },
                #{ dogGumPayDate },
                #{ dogItemName }
                #{ memDogGum },
                 #{ memCode },
            )
    </insert>


</mapper>


